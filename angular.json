{
  "version": 1,
  "projects": {
    "thedoc": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/thedoc",
      "sourceRoot": "apps/thedoc/src",
      "prefix": "thestack",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/thedoc",
            "index": "apps/thedoc/src/index.html",
            "main": "apps/thedoc/src/main.ts",
            "polyfills": "apps/thedoc/src/polyfills.ts",
            "tsConfig": "apps/thedoc/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/thedoc/src/favicon.ico", "apps/thedoc/src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/thedoc/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/thedoc/src/environments/environment.ts",
                  "with": "apps/thedoc/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "thedoc:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "thedoc:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "thedoc:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/thedoc/tsconfig.app.json",
              "apps/thedoc/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/thedoc/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/thedoc/jest.config.js",
            "tsConfig": "apps/thedoc/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/thedoc/src/test-setup.ts"
          }
        }
      }
    },
    "thedoc-e2e": {
      "root": "apps/thedoc-e2e",
      "sourceRoot": "apps/thedoc-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/thedoc-e2e/cypress.json",
            "tsConfig": "apps/thedoc-e2e/tsconfig.e2e.json",
            "devServerTarget": "thedoc:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "thedoc:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/thedoc-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/thedoc-e2e/**"]
          }
        }
      }
    },
    "chat": {
      "projectType": "library",
      "root": "libs/chat",
      "sourceRoot": "libs/chat/src",
      "prefix": "thestack",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/chat/tsconfig.lib.json",
              "libs/chat/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/chat/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/chat/jest.config.js",
            "tsConfig": "libs/chat/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/chat/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "shared-modules": {
      "projectType": "library",
      "root": "libs/shared/modules",
      "sourceRoot": "libs/shared/modules/src",
      "prefix": "thestack",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/shared/modules/tsconfig.lib.json",
              "libs/shared/modules/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/shared/modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/shared/modules/jest.config.js",
            "tsConfig": "libs/shared/modules/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/shared/modules/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "shared-services": {
      "projectType": "library",
      "root": "libs/shared/services",
      "sourceRoot": "libs/shared/services/src",
      "prefix": "thestack",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/shared/services/tsconfig.lib.json",
              "libs/shared/services/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/shared/services/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/shared/services/jest.config.js",
            "tsConfig": "libs/shared/services/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/shared/services/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "thedoc"
}
